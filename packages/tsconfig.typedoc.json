    },
    {
      "path": "./ipydrawio"
    },
    {
      "path": "./ipydrawio-notebook/src"
    },
    {
      "path": "./ipydrawio-notebook"
    },
    {
      "path": "./ipydrawio-pdf/src"
    },
    {
      "path": "./ipydrawio-pdf"
    }
  ]
}
