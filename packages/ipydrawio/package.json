{
  "author": "Dead Pixels Collective",
  "bugs": {
    "url": "https://github.com/deathbeds/ipydrawio/issues"
  },
  "dependencies": {
    "@jupyterlab/application": "3"
  },
  "description": "A JupyterLab extension for embedding drawio / mxgraph.",
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.1",
    "typescript": "~3.9.3"
  },
  "peerDependencies": {
    "@deathbeds/ipydrawio-webpack": "^13.6.10-alpha2"
  },
  "files": [
    "{lib,schema,style,src}/**/*.{ts,tsx,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,css}",
    "LICENSE.txt"
  ],
  "homepage": "https://github.com/deathbeds/ipydrawio",
  "jupyterlab": {
    "extension": "lib/plugin.js",
    "mimeExtension": "lib/mime.js",
